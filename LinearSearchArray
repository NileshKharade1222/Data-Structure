public class LinearSearchArray {
    public static void main(String[] args) {
        int[] numbers = {5, 2, 9, 1, 3};
        int target = 9;

        boolean found = false;
        for (int num : numbers) {
            if (num == target) {
                found = true;
                break;
            }
        }

        if (found)
            System.out.println("Element " + target + " found in the array.");
        else
            System.out.println("Element " + target + " not found.");
    }
}
