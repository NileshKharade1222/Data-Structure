import java.util.*;
class Main {
    public static void main(String[] args) {
        
        int a,b,c;
        Scanner sc= new Scanner(System.in);
        System.out.println("Enter Two  Numbers");
        
        a=sc.nextInt();
        b=sc.nextInt();
        
        System.out.println("before swapping"+a+" "+b);
        
        c=a;
        a=b;
        b=c;
        System.out.println("after swapping"+a+" "+b);
    
        }
}
